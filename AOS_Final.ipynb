    "#Datenstrukturen\n",
    "@dataclass\n",
    "#Ein Block ist ein von SortDet/SortDetRev verwaltetes Teilintervall, Blöcke werden in einer Half von innen nach außen angeordnet.\n",
    "class Block:\n",
    "    start: int   #inklusive\n",
    "    end:   int   #exklusive\n",
    "    dirL:  bool  #Direction, True = linke Hälfte (mit SortDetRev), False = rechte Hälfte (mit SortDet)\n",
    "    sd:    SortDet  #SortDet/SortDetRev-Instanz, die [start:end) füllt\n",
    "    used:  int #Anzahl bereits gefüllter Zellen des Blocks\n",
    "\n",
    "    @property\n",
    "    def length(self) -> int:\n",
    "        return self.end - self.start\n",
    "\n",
    "    @property\n",
    "    def free(self) -> int:\n",
    "        return self.length - self.used\n",
    "\n",
    "    def insert(self, x: float) -> bool:\n",
    "        ok = self.sd.insert(x)\n",
    "        if ok:\n",
    "            self.used += 1\n",
    "        return ok\n",
    "\n",
    "    def is_empty(self) -> bool:\n",
    "        return self.used == 0\n",
    "\n",
    "\n",
    "#Half ist linke/rechte Hälfte eines Superbuckets\n",
    "class Half:\n",
    "    def __init__(self, A: List[Optional[float]], start: int, end: int, is_left: bool, q: int):\n",
    "        self.A = A\n",
    "        self.is_left = is_left\n",
    "        self.blocks: List[Block] = []\n",
    "        length = end - start\n",
    "        if length <= 0:\n",
    "            return\n",
    "\n",
    "        #1) Globale Partition der Hälfte, um Subarray-Grenzen zu erhalten (Instanz wird nicht zum Füllen genutzt)\n",
    "        sd_full = SortDetRev(A, start, end) if is_left else SortDet(A, start, end)\n",
    "\n",
    "        #2) Erzeuge je Subarray-Grenze einen Block mit eigener SortDet/SortDetRev-Instanz\n",
    "        tmp_blocks: List[Block] = []\n",
    "        for (s, e) in sd_full.subarrays:\n",
    "            sd_block = SortDetRev(A, s, e) if is_left else SortDet(A, s, e)\n",
    "            tmp_blocks.append(Block(start=s, end=e, dirL=is_left, sd=sd_block, used=0))\n",
    "\n",
    "        #3) Reihenfolge innen -> außen herstellen auf linker Seite\n",
    "        if is_left:\n",
    "            tmp_blocks.reverse()\n",
    "\n",
    "        self.blocks = tmp_blocks\n",
    "\n",
    "        #Für Logging/Debugging bzw. Value-guided Version, q(Median der Blocklängen) aus Blocklänge ableiten, IGNORIEREN\n",
    "        self.block_unit = max(1, int(sorted((b.end - b.start) for b in self.blocks)[len(self.blocks)//2]))\n",
    "        \n",
    "    def capacity(self) -> int:\n",
    "        return sum(b.length for b in self.blocks)\n",
    "\n",
    "    def block_size(self) -> int:\n",
    "        return getattr(self, \"block_unit\", 1)\n",
    "\n",
    "    def used(self) -> int:\n",
    "        return sum(b.used for b in self.blocks)\n",
    "\n",
    "    def free(self) -> int:\n",
    "        return self.capacity() - self.used()\n",
    "\n",
    "    #Verscuht x in den innsersten noch nicht vollen Block zu legen, dann schrittweise nach Außen\n",
    "    def insert(self, x: float) -> bool:\n",
    "        for b in self.blocks:\n",
    "            if b.free > 0 and b.insert(x):#delegiert an sd\n",
    "                return True\n",
    "        return False\n",
    "\n",
    "\n",
    "    #Vom äußeren Rand der Half leere Blöcke abgeben\n",
    "    def pop_outer_empty_blocks(self, how_many_cells: int) -> List[Block]:\n",
    "        taken: List[Block] = []\n",
    "        remain = how_many_cells\n",
    "        while self.blocks and remain > 0:\n",
    "            b = self.blocks[-1]\n",
    "            if not b.is_empty():\n",
    "                break\n",
    "            if b.length > remain:\n",
    "                break\n",
    "            taken.append(b)\n",
    "            self.blocks.pop()\n",
    "            remain -= b.length\n",
    "        return taken\n",
    "\n",
    "    #Leere Blöcke am Rand anhängen, SortDet/Rev-Instanz neu aufspannen (weil Richtungswechsel) \n",
    "    def push_outer_blocks(self, new_blocks: List[Block]):\n",
    "        for b in new_blocks:\n",
    "            if b.dirL != self.is_left:\n",
    "                if self.is_left:\n",
    "                    b.sd = SortDetRev(self.A, b.start, b.end)\n",
    "                else:\n",
    "                    b.sd = SortDet(self.A, b.start, b.end)\n",
    "                b.dirL = self.is_left\n",
    "            self.blocks.append(b)  \n",
    "\n",
    "    #Bis zu how_many_cells leere Zellen vom äußeren Rand nehmen, ggF splitten, gibt Liste leerer Bläcke wieder\n",
    "    def pop_outer_empty_cells(self, how_many_cells: int, unit: int) -> List[Block]:\n",
    "        taken: List[Block] = []\n",
    "        remain = how_many_cells\n",
    "    \n",
    "        while self.blocks and remain > 0:\n",
    "            b = self.blocks[-1]\n",
    "            if not b.is_empty():\n",
    "                break\n",
    "    \n",
    "            if b.length <= remain:\n",
    "                taken.append(b)\n",
    "                self.blocks.pop()\n",
    "                remain -= b.length\n",
    "                continue\n",
    "    \n",
    "            # Block ist größer als Bedarf -> splitte in Vielfache von 'unit'\n",
    "            take_len = (remain // unit) * unit\n",
    "            if take_len <= 0:\n",
    "                break\n",
    "    \n",
    "            new_start = b.end - take_len\n",
    "            new_end   = b.end\n",
    "            new_sd = SortDetRev(self.A, new_start, new_end) if self.is_left else SortDet(self.A, new_start, new_end)\n",
    "            new_block = Block(new_start, new_end, self.is_left, new_sd, used=0)\n",
    "            taken.append(new_block)\n",
    "    \n",
    "            # Restblock einkürzen + sd neu aufspannen\n",
    "            b.end = new_start\n",
    "            b.sd = SortDetRev(self.A, b.start, b.end) if self.is_left else SortDet(self.A, b.start, b.end)\n",
    "            remain -= take_len\n",
    "            break \n",
    "    \n",
    "        return taken\n",
    "\n",
    "#Superbucket (linke & rechte Half um Achse in der Mitte), kapselt Einfügelogik (gibt hier noch value-guided Option, kann ignoriert werden) \n",
    "from dataclasses import dataclass\n",
    "@dataclass\n",
    "class Superbucket:\n",
    "    left:  Half\n",
    "    right: Half\n",
    "    t: float = 0.5           #Entscheidungsgrenze (threshold) im lokalen [0,1]-Bucket-Intervall\n",
    "    alpha: float = 0.2         #Nur für wertegeleitete Option: Glättung für das Update t <- (1-alpha) t + alpha p* \n",
    "    min_t: float = 0.1       # Guardrails\n",
    "    max_t: float = 0.9\n",
    "    snap_t_to_blocks: bool = True \n",
    "\n",
    "    def capacity(self) -> int: return self.left.capacity() + self.right.capacity()\n",
    "    def used(self) -> int:     return self.left.used() + self.right.used()\n",
    "    def free(self) -> int:     return self.left.free()  + self.right.free()\n",
    "\n",
    "    #Wähle Half mit mehr freiem Platz und probiere einzufügen\n",
    "    def insert_middle_pref(self, x: float) -> bool:\n",
    "        if self.used() == 0:\n",
    "            return self.left.insert(x)\n",
    "        target = self.left if self.left.free() >= self.right.free() else self.right\n",
    "        if target.insert(x):\n",
    "            return True\n",
    "        other = self.right if target is self.left else self.left\n",
    "        return other.insert(x)\n",
    "\n",
    "    #Wertegeführtes Einfügen in Half, t ist threshold, wird adaptiv angepasst, IGNORIEREN\n",
    "    def insert_value_guided(self, x: float, i: int, K: int) -> bool:\n",
    "        #berechne lokalen Wert in [0,1] für Superbucket i\n",
    "        x_local = K * max(0.0, min(1.0, x)) - i\n",
    "        go_left = (x_local <= self.t)\n",
    "        if go_left:\n",
    "            if self.left.insert(x) or self.right.insert(x):\n",
    "                self._update_t_from_free()\n",
    "                return True\n",
    "        else:\n",
    "            if self.right.insert(x) or self.left.insert(x):\n",
    "                self._update_t_from_free()\n",
    "                return True\n",
    "        return False\n",
    "
    "    #Für wertegeleitete Option: Update von t: p* = L_frei / (L_frei + R_frei), t <- (1-alpha)·t + alpha * p*, IGNORIEREN\n",
    "    def _update_t_from_free(self):\n",
    "        Lf = self.left.free()\n",
    "        Rf = self.right.free()\n",
    "        tot = Lf + Rf\n",
    "        if tot <= 0:\n",
    "            return\n",
    "        p_star = Lf / tot\n",
    "        self.t = (1 - self.alpha) * self.t + self.alpha * p_star\n",
    "        self.t = min(self.max_t, max(self.min_t, self.t))\n",
    "        #AufBlockraster snappen für stabile Grenze\n",
    "        if self.snap_t_to_blocks:\n",
    "            cap = self.capacity()\n",
    "            if cap > 0:\n",
    "                #Grobes Raster aus Blockeinheiten, nimm das kleinere Blockmaß (unter den aktuellen Bedingungen nciht relevant, weil 1)\n",
    "                qL = getattr(self.left,  'block_unit', 1)\n",
    "                qR = getattr(self.right, 'block_unit', 1)\n",
    "                step = max(1, min(qL, qR)) / cap\n",
    "                k = round(self.t / step)\n",
    "                self.t = min(self.max_t, max(self.min_t, k * step))\n",
    "\n"
   ]
