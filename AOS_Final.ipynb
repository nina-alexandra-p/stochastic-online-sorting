    "# Adaptive Online Sorter (AOS) mit dynamischem Kapazitätsausgleich\n",
    "#Vorderer Teil heißt U, wird in K Superbuckets geteilt, hinterer Teil: B\n",
    "#3 unterschiedliche \"Meta\"-Phasen: U befüllen, B befüllen, im Notfall Lücken in U füllen\n",
    "class AOS:\n",
    "    def __init__(self, \n",
    "                 n: int, \n",
    "                 c_end: int = 5, \n",
    "                 debug: bool = True, \n",
    "                 ring: bool = True,  \n",
    "                 use_value_guided: bool = False,     \n",
    "                 t_alpha: float = 0.2,                \n",
    "                 t_min: float = 0.1,                  \n",
    "                 t_max: float = 0.9,                 \n",
    "                 snap_t_to_blocks: bool = True):\n",
    "        assert n > 0\n",
    "        self.n = n\n",
    "        self.A: List[Optional[float]] = [None] * n\n",
    "        self.c_end = c_end\n",
    "        self.rounds = 0\n",
    "        self.debug = debug\n",
    "        self.ring = ring\n",
    "\n",
    "        #Flag wird gesetzt, wenn Rebalancing nicht mehr sinnvoll/zulässig ist\n",
    "        self.blocked_until_next_overflow = False\n",
    "        self.block_reason = None\n",
    "\n",
    "        self.use_value_guided = use_value_guided\n",
    "        self.t_alpha = t_alpha #Glättung\n",
    "        self.t_min = t_min\n",
    "        self.t_max = t_max\n",
    "        self.snap_t_to_blocks = snap_t_to_blocks #auf Blockraster runden \n",
    "\n",
    "        #Logs fürs Debugging\n",
    "        self.q_history: List[int] = []\n",
    "        self.overflow_log: List[dict] = []\n",
    "        self.rebalance_log: List[dict] = []\n",
    "\n",
    "        #Größe Backyard = c_end·(log2 n)^2, falls |B|> |A| wird gekappt auf (log2 n)^2\n",
    "        Llog = max(1.0, math.log2(max(2, n)))\n",
    "        self.B_len = max(1, int(c_end * (Llog ** 2)))\n",
    "        if self.B_len >= n:\n",
    "            self.B_len = max(1, int(Llog ** 2))\n",
    "        self.B_start = n - self.B_len\n",
    "\n",
    "        self.l, self.K = self._choose_level_polylog(n)\n",
    "        U_start, U_end = 0, self.B_start\n",
    "        U_len = U_end - U_start\n",
    "\n",
    "        #Blockmaß Einheit für Logging (und alt)\n",
    "        superb_len_base = U_len // self.K if self.K else U_len\n",
    "        self.q = max(1, superb_len_base // 4)\n",
    "        self.q_history.append(self.q)\n",
    "\n",
    "        #Aktive Teile: Array in K Superbuckets teilen mit je zwei Halfes, jede Half hat SD/SDRev-Blöcke\n",
    "        self.S: List[Superbucket] = []\n",
    "        parts = self._partition_equal(U_start, U_end, self.K)\n",
    "        for (s, e) in parts:\n",
    "            mid = s + (e - s) // 2\n",
    "            left  = Half(self.A, s,   mid, is_left=True,  q=self.q)\n",
    "            right = Half(self.A, mid, e,   is_left=False, q=self.q)\n",
    "            sb = Superbucket(left, right)\n",
    "            #Parametrisierung, nur relevant für wertgeleitet, IGNORIEREN\n",
    "            sb.alpha = self.t_alpha\n",
    "            sb.min_t = self.t_min\n",
    "            sb.max_t = self.t_max\n",
    "            sb.snap_t_to_blocks = self.snap_t_to_blocks\n",
    "            self.S.append(sb)\n",
    " \n",
    "        self.groups: List[List[int]] = [[i] for i in range(self.K)] \n",
    "        self.group_of: List[int] = list(range(self.K)) # Mapping: Bucket-Index -> Gruppen-ID\n",
    "        self.merge_level: int = 0 \n",
    "        self.min_groups_after_merge = 1 #bis wohin mergen\n",
    "\n",
    "        #Zähler, insb wichitg für Einfügen in Lücken nach Fail\n",
    "        self.B_sortdet = SortDet(self.A, self.B_start, self.n, label=\"B\")\n",
    "        self.B_used: int = 0\n",
    "        self.B_full: bool = False\n",
    "        self.placed_in_U_after_B_full: int = 0\n",
    "        self.tick: int = 0\n",
    "        self.phase = Phase.U_FILL\n",
    "        self.free_U_count = self.B_start \n",
    "        self.next_free_U = 0\n",
    "\n",
    "    #Verringert Zähler für ersten Arrayteil\n",
    "    def _dec_free_U(self, k=1):\n",
    "        self.free_U_count -= k \n",
    "        assert self.free_U_count >= 0\n",
    "    #Erhöht Zähler für ersten Arrayteil\n",
    "    def _inc_free_U(self, k=1): \n",
    "        self.free_U_count += k\n",
    "        assert self.free_U_count <= self.B_start\n",
    "        \n",
    "    #Wählt l, und K = 2^l so, dass K in Größenordung Theta(n/log^2 n)\n",
    "    def _choose_level_polylog(self, n: int) -> Tuple[int, int]:\n",
    "        t = max(1.0, math.log2(max(2, n)))\n",
    "        denom = t * t\n",
    "        L = n / (4 * denom)\n",
    "        U = n / (2 * denom)\n",
    "        seed = n / (3 * denom)\n",
    "        ell = int(math.floor(math.log2(max(2.0, seed))))\n",
    "        while (1 << ell) <= L: ell += 1\n",
    "        while ell > 0 and (1 << ell) >= U: ell -= 1\n",
    "        K = max(1, 1 << max(0, ell))\n",
    "        return max(0, ell), K\n",
    "        \n",
    "    #Teilt in gleich große Intervalle\n",
    "    def _partition_equal(self, s: int, e: int, k: int) -> List[Tuple[int,int]]:\n",
    "        if k <= 0: return [(s, e)]\n",
    "        n = e - s\n",
    "        base = n // k\n",
    "        rem  = n %  k\n",
    "        out = []\n",
    "        p = s\n",
    "        for i in range(k):\n",
    "            q = p + base + (1 if i < rem else 0)\n",
    "            out.append((p, q))\n",
    "            p = q\n",
    "        return out\n",
    "        \n",
    "\n",
    "    #Mappt x aus [0,1] auf Superbucket-Index \n",
    "    def _i_of_x(self, x: float) -> int:\n",
    "        i = int(math.ceil(self.K * max(0.0, min(1.0, x))))\n",
    "        if i < 1: i = 1\n",
    "        if i > self.K: i = self.K\n",
    "        return i - 1\n",
    "\n",
    "    #Für Debugging/Logging\n",
    "    def _snapshot_half(self, half: Half) -> dict:\n",
    "        return {\n",
    "            \"blocks_len\":  [b.length for b in half.blocks],\n",
    "            \"blocks_used\": [b.used   for b in half.blocks],\n",
    "            \"capacity\":    half.capacity(),\n",
    "            \"used\":        half.used(),\n",
    "            \"free\":        half.free(),\n",
    "            \"num_blocks\":  len(half.blocks),\n",
    "            \"is_left\":     half.is_left,\n",
    "        }\n",
    "\n",
    "    def _snapshot_state(self) -> List[dict]:\n",
    "        out = []\n",
    "        for idx, Sb in enumerate(self.S):\n",
    "            out.append({\n",
    "                \"idx\": idx,\n",
    "                \"left\":  self._snapshot_half(Sb.left),\n",
    "                \"right\": self._snapshot_half(Sb.right),\n",
    "                \"free_total\": Sb.free(),\n",
    "                \"used_total\": Sb.used(),\n",
    "                \"capacity\":   Sb.capacity(),\n",
    "            })\n",
    "        return out\n",
    "\n",
    "    #Führt insert auf Eingabesequenz aus und sammelt Statusinfos für Report\n",
    "    def run_with_report(self, sequence: List[float]) -> Tuple[List[Optional[float]], dict]:\n",
    "        for x in sequence:\n",
    "            self.insert(x)\n",
    "        B_filled = self.B_used\n",
    "        report = {\n",
    "            \"n\": self.n,\n",
    "            \"ell\": self.l,\n",
    "            \"K\": self.K,\n",
    "            \"q_history\": self.q_history[:],\n",
    "            \"rounds\": self.rounds,\n",
    "            \"overflow_log\": self.overflow_log[:],\n",
    "            \"rebalance_log\": self.rebalance_log[:],\n",
    "            \"B\": {\n",
    "                \"start\": self.B_start,\n",
    "                \"len\": self.B_len,\n",
    "                \"filled\": B_filled,\n",
    "                \"free\": self.B_len - B_filled,\n",
    "                \"full\": self.B_full,\n",
    "                \"placed_in_U_after_B_full\": self.placed_in_U_after_B_full,\n",
    "            },\n",
    "            \"final_filled\": sum(1 for v in self.A if v is not None),\n",
    "        }\n",
    "        return self.A, report\n",
    "        \n",
    "    #Zentrale API: Erst Phase U_FILL (mit Rebalance und Merge und Rebalance), dann B_FILL, ggf. U_AFTER_B (=Emergency Fill)\n",
    "    def insert(self, x: float) -> bool:\n",
    "        self.tick += 1 #timestamp\n",
    "    \n",
    "        #Phase 2 oder 3: keine Rebalances/Merges mehr -> direkt B/U-Pfad.\n",
    "        if self.phase is Phase.B_FILL or self.phase is Phase.U_AFTER_B:\n",
    "            return self._put_into_B_or_U_phase(x)\n",
    "    \n",
    "        #Phase 1: Mapping -> Ziel-Superbucket\n",
    "        i = self._i_of_x(x)\n",
    "        Sb = self.S[i]\n",
    "    \n",
    "        #Option wertgeführt vs. in die Hälfte mit mehr Platz\n",
    "        if (self.use_value_guided and Sb.insert_value_guided(x, i=i, K=self.K)) \\\n",
    "           or (not self.use_value_guided and Sb.insert_middle_pref(x)):\n",
    "            self._dec_free_U(1)\n",
    "            return True\n",
    "\n",
    "        #Überlauf loggen (x passt nicht in supebucket)\n",
    "        if self.debug:\n",
    "            self.overflow_log.append({\n",
    "                \"tick\": self.tick, \"x\": x, \"i\": i,\n",
    "                \"free_L\": Sb.left.free(), \"free_R\": Sb.right.free(),\n",
    "                \"q\": self.q\n",
    "            })\n",
    "        \n",
    "        #Rebalance an Gruppen-Grenzen: Verschiebt freie Außenblöcke zwischen benachbarten Superbuckets (ohne merge)\n",
    "        if any(len(g) > 1 for g in self.groups):\n",
    "            if self._rebalance_round_group_edges(unit=1):\n",
    "                if (self.use_value_guided and Sb.insert_value_guided(x, i=i, K=self.K)) \\\n",
    "                   or (not self.use_value_guided and Sb.insert_middle_pref(x)):\n",
    "                    self._dec_free_U(1)\n",
    "                    return True\n",
    "\n",
    "        #Rebalance auf allen Grenzen (berechnet Delta auf Zellebene,verschiebt freie Außenblöcke)\n",
    "        if self._rebalance_round_buckets_no_merge():\n",
    "            if (self.use_value_guided and Sb.insert_value_guided(x, i=i, K=self.K)) \\\n",
    "               or (not self.use_value_guided and Sb.insert_middle_pref(x)):\n",
    "                self._dec_free_U(1)   \n",
    "                return True\n",
    "\n",
    "        #Wenn Rebalance auf der Ebene nicht ausreichend -> Merge und Rebalance (\"Eskalation\")\n",
    "        ok_groups = self._escalate_merge_then_rebalance()\n",
    "        if not ok_groups:\n",
    "            self._switch_to_B_phase(reason=\"rebalance_group_not_feasible\")\n",
    "            return self._put_into_B_or_U_phase(x)\n",
    "            \n",
    "        #Sicherheitscheck: U voll ->Dann B befüllen\n",
    "        Ctot = sum(self.S[i].free() for i in range(self.K))\n",
    "        if Ctot == 0:\n",
    "            self._switch_to_B_phase(reason=\"U_full_after_fairness\")\n",
    "            return self._put_into_B_or_U_phase(x)\n",
    "\n",
    "        #Nach erfolgreichem Rebalance/Merge: Insert-Versuch im Ziel-Superbucket machen\n",
    "        if (self.use_value_guided and Sb.insert_value_guided(x, i=i, K=self.K)) \\\n",
    "           or (not self.use_value_guided and Sb.insert_middle_pref(x)):\n",
    "            self._dec_free_U(1)\n",
    "            return True\n",
    "\n",
    "        #In Gruppe des Superbuckets versuchen einzufügen (spiralförmig vorgehen)\n",
    "        gi = self.group_of[i]\n",
    "        if self._insert_in_group_after_fairness(gi, i, x):\n",
    "            self._dec_free_U(1)\n",
    "            return True\n",
    "\n",
    "        #Fallback (pathologisch)\n",
    "        self._switch_to_B_phase(reason=\"pathological_after_successful_group_rebalance\")\n",
    "        return self._put_into_B_or_U_phase(x)\n",
    "\n",
    "    #Nur noch B Phase, Grund wird in block_reason notiert\n",
    "    def _switch_to_B_phase(self, reason:str):\n",
    "        self.blocked_until_next_overflow = True\n",
    "        self.block_reason = {\"note\": reason}\n",
    "        self.phase = Phase.B_FILL\n",
    "        \n",
    "    #Erst B mit SortDet füllen, dann Reste in U \n",
    "    def _put_into_B_or_U_phase(self, x: float) -> bool:\n",
    "        # Phase 2: zuerst B\n",
    "        if self.phase is Phase.B_FILL:\n",
    "            if not self.B_full and self.B_sortdet.insert(x):\n",
    "                self.B_used += 1\n",
    "                if self.B_used >= self.B_len:\n",
    "                    self.B_full = True\n",
    "                    self.phase = Phase.U_AFTER_B\n",
    "                return True\n",
    "            if not self.B_full:\n",
    "                self.B_full = True\n",
    "            self.phase = Phase.U_AFTER_B\n",
    "        while self.next_free_U < self.B_start and self.A[self.next_free_U] is not None:\n",
    "            self.next_free_U += 1\n",
    "        if self.next_free_U < self.B_start:\n",
    "            assert self.A[self.next_free_U] is None\n",
    "            self.A[self.next_free_U] = x\n",
    "            self.placed_in_U_after_B_full += 1\n",
    "            self._dec_free_U(1)\n",
    "            return True\n",
    "        return False  #kein Platz mehr in U\n",
    "\n",
    "\n",
    "    #Rebalance Runde auf Superbucket Grenzen, Delta wird zellgenau berechnet, reale Kapazitäten einbezogen und Plan erzeugt\n",
    "    #Wenn es einen Plan gibt (also machbar), dann True wiedergeben\n",
    "    def _rebalance_round_buckets_no_merge(self, unit: int = 1) -> bool:\n",
    "        # Delta ist zellgenau\n",
    "        C = [self.S[i].free() for i in range(self.K)]\n",
    "        _, _, Delta = self._delta_unit(unit=1)\n",
    "        if all(d == 0 for d in Delta):\n",
    "            return True  \n",
    "        #Falls genau ein Bucket das Zellen braucht ->Seine Grenzen priorisieren für Plan\n",
    "        i_focus = None\n",
    "        if self.overflow_log:\n",
    "            i_focus = self.overflow_log[-1][\"i\"]\n",
    "    \n",
    "        plan_all, meta = self._plan_all_boundaries_ring_with_caps(\n",
    "            Delta, unit=unit, ring_averse=True, i_focus=i_focus\n",
    "        )\n",
    "    \n",
    "        alpha = float(meta.get(\"alpha\", 0.0))\n",
    "        need_remaining = int(meta.get(\"need_remaining\", 0))\n",
    "        feasible = (need_remaining == 0 and alpha >= 1.0 - 1e-12 and bool(plan_all))\n",
    "        if not feasible:\n",
    "            return False\n",
    "    \n",
    "        #Plan anwenden:Freie Außenblöcke über die Grenzen verschieben\n",
    "        for (b, direction, cells) in plan_all:\n",
    "            if direction == 'right':\n",
    "                self._pop_right_push_left_across_boundary(b, cells, unit=unit)\n",
    "            else:\n",
    "                self._pop_left_push_right_across_boundary(b, cells, unit=unit)\n",
    "    \n",
    "        self.rounds += 1\n",
    "        if self.debug:\n",
    "            self.rebalance_log.append({\n",
    "                \"round\": self.rounds,\n",
    "                \"q_before\": self.q, \"q_after\": self.q,\n",
    "                \"C\": C, \"Ctot\": sum(C),\n",
    "                \"Delta\": Delta,\n",
    "                \"plan\": [{\"boundary\": b, \"dir\": d, \"cells\": c} for (b, d, c) in plan_all],\n",
    "                \"note\": \"applied_single_round_no_merge\",\n",
    "                \"meta\": meta,\n",
    "            })\n",
    "        self._refresh_all_ts()\n",
    "        return True\n",
    "        \n",
    "    #Rebalance-Runde auf Gruppenebene \n",
    "    def _rebalance_round_group_edges(self, unit: int = 1) -> bool:\n",
    "        if self.phase is not Phase.U_FILL:\n",
    "            return False\n",
    "        Cg, _, Delta_g = self._group_delta_unit(unit=unit)\n",
    "        if all(d == 0 for d in Delta_g):\n",
    "            return True\n",
    "        i_focus = None\n",
    "        if self.overflow_log:\n",
    "            last_i = self.overflow_log[-1][\"i\"]\n",
    "            i_focus = self.group_of[last_i]\n",
    "    \n",
    "        plan, meta = self._plan_groups_with_caps(Delta_g, unit=unit, ring_averse=True, i_focus=i_focus)\n",
    "    \n",
    "        alpha = float(meta.get(\"alpha\", 0.0))\n",
    "        need_remaining = int(meta.get(\"need_remaining\", 0))\n",
    "        feasible = bool(plan) and (alpha >= 1.0 - 1e-12) and (need_remaining == 0)\n",
    "        if not feasible:\n",
    "            return False\n",
    "    \n",
    "        applied = self._apply_group_plan(plan, unit=unit)\n",
    "        self.rounds += 1\n",
    "    \n",
    "        if self.debug:\n",
    "            self.rebalance_log.append({\n",
    "                \"round\": self.rounds,\n",
    "                \"note\": \"applied_one_round_group_edges_cell_level\",\n",
    "                \"Delta_g\": Delta_g,\n",
    "                \"plan_g\": [{\"g\": g, \"dir\": d, \"cells\": c} for (g,d,c) in plan],\n",
    "                \"applied_g\": applied,\n",
    "            })\n",
    "        self._refresh_all_ts()\n",
    "        return True\n",
    "        \n",
    "    #Wiederholt Rebalance-Runden, bis Gruppen-Fairness erreicht oder blockiert. Wenn insgesamt nicht möglich, abbrechen-> False ausgeben\n",
    "    def _rebalance_group_or_block(self) -> bool:\n",
    "        max_subrounds = 200\n",
    "        if self.phase is not Phase.U_FILL:\n",
    "            return False\n",
    "\n",
    "        for _ in range(max_subrounds):\n",
    "            C = [self.S[i].free() for i in range(self.K)]\n",
    "            _, _, Delta = self._delta_unit(unit=1)\n",
    "\n",
    "            # Erfolgskriterium abhängig vom Merge-Stand\n",
    "            if len(self.groups) > 1:\n",
    "                _, _, Delta_g = self._group_delta_unit(unit=1)\n",
    "                if all(d == 0 for d in Delta_g):\n",
    "                    return True\n",
    "            else:\n",
    "                return True\n",
    "\n",
    "            # Fokus = eindeutig positiver Delta-Bucket\n",
    "            pos_idx = [i for i, d in enumerate(Delta) if d > 0]\n",
    "            i_focus = pos_idx[0] if len(pos_idx) == 1 else None\n",
    "\n",
    "            # Plan berechnen (zellgenau, ring-avers)\n",
    "            plan_all, meta = self._plan_all_boundaries_ring_with_caps(\n",
    "                Delta, unit=1, ring_averse=True, i_focus=i_focus\n",
    "            )\n",
    "\n",
    "            alpha = float(meta.get(\"alpha\", 0.0))\n",
    "            need_remaining = int(meta.get(\"need_remaining\", 0))\n",
    "            feasible = (need_remaining == 0 and alpha >= 1.0 - 1e-12 and bool(plan_all))\n",
    "            if not feasible:\n",
    "                ok2 = self._escalate_merge_then_rebalance()\n",
    "                if not ok2:\n",
    "                    self.blocked_until_next_overflow = True\n",
    "                    self.block_reason = {\n",
    "                        \"note\": meta.get(\"note\", \"blocked_after_full_merge\"),\n",
    "                        \"alpha\": alpha,\n",
    "                        \"need_remaining\": need_remaining,\n",
    "                        \"Delta\": Delta,\n",
    "                        \"capR\": meta.get(\"capR\"), \"capL\": meta.get(\"capL\"),\n",
    "                    }\n",
    "                    if self.debug:\n",
    "                        snap = self._snapshot_state()\n",
    "                        self.rebalance_log.append({\n",
    "                            \"round\": self.rounds + 1,\n",
    "                            \"q_before\": self.q, \"q_after\": self.q,\n",
    "                            \"C\": C, \"Ctot\": sum(C),\n",
    "                            \"Delta_raw\": Delta, \"Delta\": Delta,\n",
    "                            \"plan\": [], \"applied_moves\": [],\n",
    "                            \"snapshot_before\": snap,\n",
    "                            \"snapshot_after\": snap,\n",
    "                            \"note\": \"blocked_no_full_fairness_after_full_merge\",\n",
    "                            \"meta\": meta,\n",
    "                            \"groups\": [g[:] for g in self.groups],\n",
    "                            \"merge_level\": self.merge_level,\n",
    "                        })\n",
    "                    return False\n",
    "                return True\n",
    "\n",
    "            #Plan anwenden und Logging/Snapshot\n",
    "            snap_before = self._snapshot_state() if self.debug else None\n",
    "            applied_moves = []\n",
    "            for (b, direction, cells) in plan_all:\n",
    "                if direction == 'right':\n",
    "                    moved = self._pop_right_push_left_across_boundary(b, cells, unit=1)\n",
    "                else:\n",
    "                    moved = self._pop_left_push_right_across_boundary(b, cells, unit=1)\n",
    "                if moved > 0:\n",
    "                    applied_moves.append({\n",
    "                        \"boundary\": b, \"dir\": direction,\n",
    "                        \"cells_req\": cells, \"cells_moved\": moved\n",
    "                    })\n",
    "\n",
    "            self.rounds += 1\n",
    "            if self.debug:\n",
    "                self.rebalance_log.append({\n",
    "                    \"round\": self.rounds,\n",
    "                    \"q_before\": self.q, \"q_after\": self.q,\n",
    "                    \"C\": C, \"Ctot\": sum(C),\n",
    "                    \"Delta_raw\": Delta, \"Delta\": Delta,\n",
    "                    \"plan\": [{\"boundary\": b, \"dir\": d, \"cells\": c} for (b,d,c) in plan_all],\n",
    "                    \"applied_moves\": applied_moves,\n",
    "                    \"snapshot_before\": snap_before,\n",
    "                    \"snapshot_after\": self._snapshot_state(),\n",
    "                    \"note\": \"applied_group_exact_cell_level\",\n",
    "                    \"meta\": meta,\n",
    "                })\n",
    "\n",
    "        self.blocked_until_next_overflow = True\n",
    "        self.block_reason = {\"note\": \"max_subrounds_reached\"}\n",
    "        return False\n",
    "        \n",
    "    #Eskalationsstrategie: Wenn Rebalance auf der Ebene scheitert, merge benachbarte Gruppen (G0+G1, G2+G3, …) und versuche erneut Rebalance.\n",
    "    def _escalate_merge_then_rebalance(self, unit: int = 1, max_levels: int = 32) -> bool:\n",
    "        levels = 0\n",
    "        while True:\n",
    "            Cg, _, Delta_g = self._group_delta_unit(unit=unit)\n",
    "            if all(d == 0 for d in Delta_g):\n",
    "                return True\n",
    "    \n",
    "            # Fokus-Gruppe (letzter Overflow-Bucket)\n",
    "            i_focus = None\n",
    "            if self.overflow_log:\n",
    "                last_i = self.overflow_log[-1][\"i\"]\n",
    "                i_focus = self.group_of[last_i]\n",
    "    \n",
    "            plan, meta = self._plan_groups_with_caps(Delta_g, unit=unit, ring_averse=True, i_focus=i_focus)\n",
    "            feasible = bool(plan) and float(meta.get(\"alpha\", 0.0)) >= 1.0 - 1e-12 and int(meta.get(\"need_remaining\", 0)) == 0\n",
    "    \n",
    "            if feasible:\n",
    "                applied = self._apply_group_plan(plan, unit=unit)\n",
    "                self.rounds += 1\n",
    "                if self.debug:\n",
    "                    self.rebalance_log.append({\n",
    "                        \"round\": self.rounds,\n",
    "                        \"note\": \"applied_group_level\",\n",
    "                        \"groups\": [g[:] for g in self.groups],\n",
    "                        \"Delta_g\": Delta_g,\n",
    "                        \"plan_g\": [{\"g\": g, \"dir\": d, \"cells\": c} for (g,d,c) in plan],\n",
    "                        \"applied_g\": applied,\n",
    "                    })\n",
    "                self._refresh_all_ts()\n",
    "                return True\n",
    "            #Abbruchkriterien\n",
    "            if len(self.groups) <= self.min_groups_after_merge or levels >= max_levels:\n",
    "                return False\n",
    "    \n",
    "            if len(self.groups) == 1 or levels >= max_levels:\n",
    "                return False\n",
    "            self._merge_adjacent_groups()\n",
    "            levels += 1\n",
    "    \n",
    "            if self.debug:\n",
    "                self.rebalance_log.append({\n",
    "                    \"round\": self.rounds,\n",
    "                    \"note\": \"escalate_merge_level\",\n",
    "                    \"merge_level\": self.merge_level,\n",
    "                    \"new_groups\": [g[:] for g in self.groups],\n",
    "                })\n",
    "    #Test: Haben alle Superbuckets (+/-1) gleiche freie Kapazität?\n",
    "    def _is_fair(self) -> bool:\n",
    "        \"\"\"Fair = alle Superbuckets haben (±1) gleiche freie Kapazität (zellgenau).\"\"\"\n",
    "        _, _, Delta = self._delta_unit(unit=1)\n",
    "        return all(d == 0 for d in Delta)\n",
    "\n",
    "    #Aktualisiert Schwellen t der Superbuckets,nur bei value-guided, IGNORIEREN\n",
    "    def _refresh_all_ts(self):\n",
    "        if not self.use_value_guided:\n",
    "            return\n",
    "        for Sb in self.S:\n",
    "            Sb._update_t_from_free()\n",
    "\n",
    "    #Delta je Superbucket (Ziel–Ist) mit Gesamtrest Ctot auf K verteilen. (Blockweise Moves möglich über unit-steuerung-> benutzen wir aber nicht)\n",
    "    #Liefert Ziel-Ist-Differenzen Delta, die ein Rebalance ausgleichen soll\n",
    "    def _delta_unit(self, unit: int = 1):\n",
    "        C = [self.S[i].free() for i in range(self.K)]\n",
    "        Ctot = sum(C)\n",
    "        base = Ctot // self.K\n",
    "        r = Ctot - base * self.K\n",
    "    \n",
    "        order = sorted(range(self.K), key=lambda i: C[i], reverse=True)\n",
    "        plus_set = set(order[:r])  # r Buckets bekommen eine Zelle mehr\n",
    "    \n",
    "        targets = [base + (1 if i in plus_set else 0) for i in range(self.K)]\n",
    "        Delta_raw = [targets[i] - C[i] for i in range(self.K)]\n",
    "    \n",
    "        if unit > 1:\n",
    "            Delta = [unit * int(round(d / unit)) for d in Delta_raw]\n",
    "            s = sum(Delta)\n",
    "            if s != 0:\n",
    "                d = Delta[:]\n",
    "                while s != 0:\n",
    "                    j = max(range(len(d)), key=lambda idx: abs(d[idx]))\n",
    "                    if s > 0:\n",
    "                        d[j] -= unit; s -= unit\n",
    "                    else:\n",
    "                        d[j] += unit; s += unit\n",
    "                Delta = d\n",
    "        else:\n",
    "            Delta = Delta_raw  # zellgenau\n",
    "    \n",
    "        return C, base, Delta\n",
    "        \n",
    "    #Analog zu _delta_unit, aber auf Gruppenebene\n",
    "    def _group_delta_unit(self, unit: int = 1):\n",
    "        Cg = self._group_free()\n",
    "        Ctot = sum(Cg)\n",
    "        G = len(Cg)\n",
    "        if G == 0 or Ctot == 0:\n",
    "            return Cg, 0, [0]*G\n",
    "        base = Ctot // G\n",
    "        r = Ctot - base * G\n",
    "        order = sorted(range(G), key=lambda gi: Cg[gi], reverse=True)\n",
    "        plus = set(order[:r])\n",
    "        targets = [base + (1 if gi in plus else 0) for gi in range(G)]\n",
    "        Delta = [targets[gi] - Cg[gi] for gi in range(G)]\n",
    "        if unit > 1:\n",
    "            Delta = [unit * int(round(d/unit)) for d in Delta]\n",
    "            s = sum(Delta)\n",
    "            if s != 0:\n",
    "                d = Delta[:]\n",
    "                while s != 0:\n",
    "                    j = max(range(len(d)), key=lambda idx: abs(d[idx]))\n",
    "                    if s > 0:\n",
    "                        d[j] -= unit; s -= unit\n",
    "                    else:\n",
    "                        d[j] += unit; s += unit\n",
    "                Delta = d\n",
    "        return Cg, base, Delta\n",
    "\n",
    "    #Kapazitäten je Grenze b in Zellen \n",
    "    def _boundary_caps(self, unit: int = 1) -> Tuple[List[int], List[int]]:\n",
    "        capR = [] #capR[b]: wie viel leere Zellen kann R_b (rechts in S_b) nach rechts abgeben? \n",
    "        capL = [] #capL[b]: wie viel leere Zellen kann L_b+1 (links in S_b+1) nach links abgeben?\n",
    "        for b in range(self.K):\n",
    "            # rechte Seite von S_b\n",
    "            Rb = self.S[b].right\n",
    "            r = 0\n",
    "            for blk in reversed(Rb.blocks):\n",
    "                if not blk.is_empty(): break\n",
    "                r += blk.length\n",
    "            # linke Seite von S_b+1\n",
    "            Lj1 = self.S[(b + 1) % self.K].left\n",
    "            l = 0\n",
    "            for blk in reversed(Lj1.blocks):\n",
    "                if not blk.is_empty(): break\n",
    "                l += blk.length\n",
    "    \n",
    "            if unit > 1:\n",
    "                r -= (r % unit)\n",
    "                l -= (l % unit)\n",
    "            capR.append(r)\n",
    "            capL.append(l)\n",
    "        return capR, capL\n",
    "\n",
    "    #Erzeugt Plan über alle Grenzen, i-focus als schneller Plan/Path, wenn nur unsere eine Grenze verschoben werden muss, sonst Projektion f_b = c - alpha*S_b mit Ring-Vermeidung\n",
    "    #Rückgabe: Liste von (boundary, direction, cells) und StatusInfos\n",
    "    def _plan_all_boundaries_ring_with_caps(\n",
    "        self,\n",
    "        Delta: List[int],\n",
    "        unit: int = 1,\n",
    "        ring_averse: bool = True,\n",
    "        i_focus: int = None,          \n",
    "    ) -> Tuple[List[Tuple[int,str,int]], dict]:\n",
    "        capR, capL = self._boundary_caps(unit=unit)\n",
    "        K = self.K\n",
    "        ring_idx = K - 1\n",
    "    \n",
    "        #Schneller Pfad\n",
    "        pos = [i for i, d in enumerate(Delta) if d > 0]\n",
    "        if i_focus is not None and len(pos) == 1 and pos[0] == i_focus:\n",
    "            need = Delta[i_focus]\n",
    "            if need <= 0:\n",
    "                return [], {\n",
    "                    \"alpha\": 1.0, \"c\": None, \"S\": None,\n",
    "                    \"flows_before_reroute\": [0]*K,\n",
    "                    \"flows\": [0]*K,\n",
    "                    \"capR\": capR, \"capL\": capL,\n",
    "                    \"need_remaining\": 0,\n",
    "                    \"note\": \"focus_no_need\",\n",
    "                }\n",
    "            b_left  = i_focus               \n",
    "            b_right = (i_focus - 1) % K     \n",
    "\n",
    "            take_left = min(capL[b_left], need); need -= take_left\n",
    "            take_right = 0\n",
    "            if need > 0:\n",
    "                take_right = min(capR[b_right], need); need -= take_right\n",
    "\n",
    "            if need > 0:\n",
    "                return [], {\n",
    "                    \"alpha\": 0.0, \"c\": None, \"S\": None,\n",
    "                    \"flows_before_reroute\": [0]*K,\n",
    "                    \"flows\": [0]*K,\n",
    "                    \"capR\": capR, \"capL\": capL,\n",
    "                    \"need_remaining\": need,\n",
    "                    \"note\": \"focus_insufficient_capacity\",\n",
    "                }\n",
    "\n",
    "            flows = [0] * K\n",
    "            plan: List[Tuple[int,str,int]] = []\n",
    "            if take_left > 0:\n",
    "                flows[b_left] -= take_left\n",
    "                plan.append((b_left, 'left', take_left))\n",
    "            if take_right > 0:\n",
    "                flows[b_right] += take_right\n",
    "                plan.append((b_right, 'right', take_right))\n",
    "\n",
    "            meta = {\n",
    "                \"alpha\": 1.0, \"c\": None, \"S\": None,\n",
    "                \"flows_before_reroute\": flows[:],\n",
    "                \"flows\": flows,\n",
    "                \"capR\": capR, \"capL\": capL,\n",
    "                \"need_remaining\": 0,\n",
    "                \"note\": \"focus_local_then_ring\",\n",
    "            }\n",
    "            return plan, meta\n",
    "\n",
    "        # Summe S_b der Deltas entlang des Rings -> Basis für die Projektion (c* alpha*S) \n",
    "        S, s = [], 0\n",
    "        for d in Delta:\n",
    "            s += d\n",
    "            S.append(s)\n",
    "        #alpha ist Projektionsparameter für Berechnung f_g = c - alpha·S[g]\n",
    "        #testet ob es ein zulässiges Intervall [lo,hi] für c gibt, sodass alle Kapazitätsgrenzen(capL_g, capR_g) eingehalten werden\n",
    "        def intersect_for_alpha(alpha: float):\n",
    "            lo = -10**18; hi = +10**18\n",
    "            for b in range(K):\n",
    "                lo_b = -capL[b] + alpha * S[b]\n",
    "                hi_b =  capR[b] + alpha * S[b]\n",
    "                if lo_b > lo: lo = lo_b\n",
    "                if hi_b < hi: hi = hi_b\n",
    "            return lo, hi   \n",
    "        lo_a, hi_a = 0.0, 1.0\n",
    "        best = None\n",
    "        for _ in range(40):\n",
    "            mid = 0.5 * (lo_a + hi_a)\n",
    "            lo_c, hi_c = intersect_for_alpha(mid)\n",
    "            if lo_c <= hi_c:\n",
    "                lo_a = mid\n",
    "                best = (mid, lo_c, hi_c)\n",
    "            else:\n",
    "                hi_a = mid\n",
    "    \n",
    "        if best is None:\n",
    "            meta = {\"alpha\": 0.0, \"c\": None, \"S\": S, \"flows\": [0]*K,\n",
    "                    \"capR\": capR, \"capL\": capL, \"note\": \"no_alpha_feasible\"}\n",
    "            return [], meta\n",
    "    \n",
    "        alpha, lo_c, hi_c = best\n",
    "        target_c = alpha * S[ring_idx]    # ring-avers\n",
    "        c = min(max(target_c, lo_c), hi_c)\n",
    "\n",
    "        #Rundet Flüsse auf unit (ist bei uns aber auf 1 gesetzt)\n",
    "        def round_unit(x: float) -> int:\n",
    "            return unit * int(round(x / unit))\n",
    "    \n",
    "        flows = [round_unit(c - alpha * S[b]) for b in range(K)]\n",
    "    \n",
    "        for b in range(K):\n",
    "            if flows[b] > capR[b]:\n",
    "                flows[b] = capR[b] - (capR[b] % unit)\n",
    "            if flows[b] < -capL[b]:\n",
    "                val = -capL[b]\n",
    "                flows[b] = val + (-val % unit) if (-val % unit) != 0 else val\n",
    "    \n",
    "        flows_before = flows[:]\n",
    "        if ring_averse:\n",
    "            flows = self._reroute_ring_flows_prefer_local(flows, capR, capL)\n",
    "    \n",
    "        plan: List[Tuple[int,str,int]] = []\n",
    "        for b, f in enumerate(flows):\n",
    "            if f > 0:\n",
    "                plan.append((b, 'right', f))\n",
    "            elif f < 0:\n",
    "                plan.append((b, 'left', -f))\n",
    "    \n",
    "        meta = {\n",
    "            \"alpha\": alpha, \"c\": c, \"S\": S,\n",
    "            \"flows_before_reroute\": flows_before,\n",
    "            \"flows\": flows,\n",
    "            \"capR\": capR, \"capL\": capL,\n",
    "            \"note\": \"projected_c_minus_alphaS\",\n",
    "        }\n",
    "        return plan, meta\n",
    "  \n",
    "    #Reduziert Verschiebungen über Ringgrenze falls möglich\n",
    "    def _reroute_ring_flows_prefer_local(self, flows: List[int], capR: List[int], capL: List[int]) -> List[int]:\n",
    "        unit = max(1, self.q)\n",
    "        ring = self.K - 1\n",
    "    \n",
    "        def take(amount):  # runde auf unit\n",
    "            return amount - (amount % unit)\n",
    "    \n",
    "        #1) Ringfluss nach rechts (>0) -> versuche über grenze b=0 nach links zu kompensieren\n",
    "        f = flows[ring]\n",
    "        if f > 0:\n",
    "            used_left_b0 = max(0, -flows[0])\n",
    "            avail_b0 = max(0, capL[0] - used_left_b0)\n",
    "            d = take(min(f, avail_b0))\n",
    "            if d > 0:\n",
    "                flows[ring] -= d\n",
    "                flows[0]    -= d\n",
    "        #Fall 2: Ringfluss nach links (<0) -> versuche über Grenze b=K-2 nach rechts zu kompensieren \n",
    "        f = flows[ring]\n",
    "        if f < 0:\n",
    "            used_right_bkm2 = max(0, flows[self.K-2])\n",
    "            avail_bkm2 = max(0, capR[self.K-2] - used_right_bkm2)\n",
    "            d = take(min(-f, avail_bkm2))\n",
    "            if d > 0:\n",
    "                flows[ring] += d\n",
    "                flows[self.K-2] += d\n",
    "        #Fall 3) S0 gibt über Ring nach links ab -> versuche b=0 nach rechts \n",
    "        f = flows[ring]\n",
    "        if f < 0:\n",
    "            used_right_b0 = max(0, flows[0])\n",
    "            avail_b0 = max(0, capR[0] - used_right_b0)\n",
    "            d = take(min(-f, avail_b0))\n",
    "            if d > 0:\n",
    "                flows[ring] += d\n",
    "                flows[0]    += d\n",
    "        #Fall 4) S_K-1 gibt über Ring nach rechts ab -> versuche b=K-2 nach links\n",
    "        f = flows[ring]\n",
    "        if f > 0:\n",
    "            used_left_bkm2 = max(0, -flows[self.K-2])\n",
    "            capL_bkm2 = capL[self.K-2]\n",
    "            avail_bkm2 = max(0, capL_bkm2 - used_left_bkm2)\n",
    "            d = take(min(f, avail_bkm2))\n",
    "            if d > 0:\n",
    "                flows[ring] -= d\n",
    "                flows[self.K-2] -= d\n",
    "    \n",
    "        return flows\n",
    "\n",
    "    #Nimmt leere Außenblöcke von R_b und hängt sie links an L_b+1 an \n",
    "    def _pop_right_push_left_across_boundary(self, b: int, cells: int, unit: int = 1) -> int:\n",
    "        iL = b\n",
    "        iR = (b + 1) % self.K\n",
    "        Rb  = self.S[iL].right\n",
    "        Lr  = self.S[iR].left\n",
    "        blocks = Rb.pop_outer_empty_cells(cells, unit=unit)\n",
    "        if blocks:\n",
    "            Lr.push_outer_blocks(blocks)\n",
    "        return sum(bk.length for bk in blocks)\n",
    "        \n",
    "    #Nimmt leere Außenblöcke von L_b+1 und hängt sie rechts an R_b an  \n",
    "    def _pop_left_push_right_across_boundary(self, b: int, cells: int, unit: int = 1) -> int:\n",
    "        \"\"\"\n",
    "        Grenze b zwischen S_b und S_{(b+1)%K}.\n",
    "        Richtung 'left': nimm leere Außenblöcke von L_{(b+1)%K} (links in S_{(b+1)%K})\n",
    "        und hänge sie rechts an R_b an.\n",
    "        \"\"\"\n",
    "        iL = b\n",
    "        iR = (b + 1) % self.K\n",
    "        Lr  = self.S[iR].left\n",
    "        Rb  = self.S[iL].right\n",
    "        blocks = Lr.pop_outer_empty_cells(cells, unit=unit)\n",
    "        if blocks:\n",
    "            Rb.push_outer_blocks(blocks)\n",
    "        return sum(bk.length for bk in blocks)\n",
    "\n",
    "    #Aktualisiert group_of\n",
    "    def _update_group_mapping(self):\n",
    "        self.group_of = [None] * self.K\n",
    "        for gid, members in enumerate(self.groups):\n",
    "            for i in members:\n",
    "                self.group_of[i] = gid\n",
    "                \n",
    "    #Fasst benachbarte Gruppen paarweise zusammen und erhöht merge level\n",
    "    def _merge_adjacent_groups(self):\n",
    "        \"\"\"Paart (G0,G1), (G2,G3), ...; bei ungerader Zahl bleibt die letzte Gruppe alleine.\"\"\"\n",
    "        new_groups = []\n",
    "        g = 0\n",
    "        while g < len(self.groups):\n",
    "            if g + 1 < len(self.groups):\n",
    "                new_groups.append(self.groups[g] + self.groups[g+1])\n",
    "                g += 2\n",
    "            else:\n",
    "                new_groups.append(self.groups[g])\n",
    "                g += 1\n",
    "        self.groups = new_groups\n",
    "        self.merge_level += 1\n",
    "        self._update_group_mapping()\n",
    "        \n",
    "    #Für jede Gruppe: linkeste Grenze und rechteste  \n",
    "    def _group_edges(self):\n",
    "        edges = []\n",
    "        for members in self.groups:\n",
    "            lo, hi = min(members), max(members)\n",
    "            edges.append((lo, hi))\n",
    "        return edges\n",
    "        \n",
    "    #Freie Zellen je Gruppe als Summe über ihre Superbuckets\n",
    "    def _group_free(self) -> List[int]:\n",
    "        return [sum(self.S[i].free() for i in g) for g in self.groups]\n",
    "        \n",
    "    #Kapazitäten an Gruppen-Grenzen analog _boundary_caps \n",
    "    def _boundary_caps_groups(self, unit: int = 1):\n",
    "        capsR, capsL = [], []\n",
    "        G = len(self.groups)\n",
    "        edges = self._group_edges()\n",
    "        for g in range(G):\n",
    "            # rechte Kante der Gruppe g\n",
    "            last_idx = edges[g][1]\n",
    "            R_last = self.S[last_idx].right\n",
    "            r = 0\n",
    "            for blk in reversed(R_last.blocks):\n",
    "                if not blk.is_empty(): break\n",
    "                r += blk.length\n",
    "    \n",
    "            # linke Kante der nächsten Gruppe\n",
    "            nxt = (g + 1) % G\n",
    "            first_idx_next = edges[nxt][0]\n",
    "            L_first_next = self.S[first_idx_next].left\n",
    "            l = 0\n",
    "            for blk in reversed(L_first_next.blocks):\n",
    "                if not blk.is_empty(): break\n",
    "                l += blk.length\n",
    "    \n",
    "            if unit > 1:\n",
    "                r -= (r % unit)\n",
    "                l -= (l % unit)\n",
    "            capsR.append(max(0, r))\n",
    "            capsL.append(max(0, l))\n",
    "        return capsR, capsL\n",
    "\n",
    "    #Gruppen-Plan analog _plan_all_boundaries_ring_with_caps nur auf Gruppen Level (schneller Pfad, sonst Projektion)\n",
    "    def _plan_groups_with_caps(self, Delta_g: List[int], unit: int = 1, ring_averse: bool = True, i_focus: int = None):\n",
    "        capR_g, capL_g = self._boundary_caps_groups(unit=unit)\n",
    "        G = len(self.groups)\n",
    "        ring_idx = G - 1\n",
    "        # Schneller Pfad nur wenn genau eine Gruppe positiven Bedarf hat\n",
    "        pos = [gi for gi, d in enumerate(Delta_g) if d > 0]\n",
    "        if i_focus is not None and len(pos) == 1 and pos[0] == i_focus:\n",
    "            need = Delta_g[i_focus]\n",
    "            g_left  = i_focus\n",
    "            g_right = (i_focus - 1) % G\n",
    "            take_left  = min(capL_g[g_left], need);  need -= take_left\n",
    "            take_right = min(capR_g[g_right], need); need -= take_right\n",
    "            if need > 0:\n",
    "                return [], {\"alpha\": 0.0, \"need_remaining\": need, \"note\": \"focus_insufficient_capacity_g\"}\n",
    "            flows = [0]*G\n",
    "            plan = []\n",
    "            if take_left  > 0: flows[g_left]  -= take_left;  plan.append((g_left,  'left',  take_left))\n",
    "            if take_right > 0: flows[g_right] += take_right; plan.append((g_right, 'right', take_right))\n",
    "            return plan, {\"alpha\": 1.0, \"flows\": flows, \"capR\": capR_g, \"capL\": capL_g, \"note\": \"focus_group\"}\n",
    "    \n",
    "        #Für Projektion\n",
    "        S = []\n",
    "        s = 0\n",
    "        for d in Delta_g:\n",
    "            s += d\n",
    "            S.append(s)\n",
    "        #alpha ist Projektionsparameter für Berechnung f_g = c - alpha·S[g]\n",
    "        #testet (über binärsuche) ob es ein zulässiges Intervall [lo,hi] für c gibt, sodass alle Kapazitätsgrenzen(capL_g, capR_g) eingehalten werden  \n",
    "        def intersect_alpha(alpha: float):\n",
    "            lo = -10**18; hi = +10**18\n",
    "            for g in range(G):\n",
    "                lo_g = -capL_g[g] + alpha * S[g]\n",
    "                hi_g =  capR_g[g] + alpha * S[g]\n",
    "                if lo_g > lo: lo = lo_g\n",
    "                if hi_g < hi: hi = hi_g\n",
    "            return lo, hi\n",
    "        lo_a, hi_a = 0.0, 1.0\n",
    "        best = None\n",
    "        for _ in range(40):\n",
    "            mid = 0.5 * (lo_a + hi_a)\n",
    "            lo_c, hi_c = intersect_alpha(mid)\n",
    "            if lo_c <= hi_c:\n",
    "                lo_a = mid\n",
    "                best = (mid, lo_c, hi_c)\n",
    "            else:\n",
    "                hi_a = mid\n",
    "        #wenn nicht machbar -> abbrechen\n",
    "        if best is None:\n",
    "            return [], {\"alpha\": 0.0, \"note\": \"no_alpha_feasible_group\", \"capR\": capR_g, \"capL\": capL_g}\n",
    "        alpha, lo_c, hi_c = best\n",
    "        # Wahl von c innerhalb des zulässigen Intervalls sodass wenig über Ringgrenze\n",
    "        c = min(max(alpha * S[ring_idx], lo_c), hi_c)\n",
    "        #Rundung der Flüsse auf Vielfache von unit (bei uns unit=1, also irrelevant)\n",
    "        def round_unit(x: float) -> int: return unit * int(round(x / unit))\n",
    "        #Projektionsformel: f_g = c - alpha·S[g] für jede Gruppengrenze g\n",
    "        flows = [round_unit(c - alpha * S[g]) for g in range(G)]\n",
    "        #Flüsse an reale Kapazitäten anpassen\n",
    "        for g in range(G):\n",
    "            if flows[g] > capR_g[g]:\n",
    "                flows[g] = capR_g[g] - (capR_g[g] % unit)\n",
    "            if flows[g] < -capL_g[g]:\n",
    "                val = -capL_g[g]\n",
    "                flows[g] = val + (-val % unit) if (-val % unit) != 0 else val\n",
    "        #Konkreten Plan mit Verschiebungen erzeugen \n",
    "        plan = []      \n",
    "        for g, f in enumerate(flows):\n",
    "            if f > 0: plan.append((g, 'right', f))\n",
    "            elif f < 0: plan.append((g, 'left', -f))\n",
    "        #Rückgabe: konkreter Plan und Status für Debugging etc.\n",
    "        return plan, {\"alpha\": alpha, \"flows\": flows, \"capR\": capR_g, \"capL\": capL_g, \"note\": \"project_group\"}\n",
    "        \n",
    "    #Wendet Gruppen-Plan auf Superbucket Grenzen an\n",
    "    def _apply_group_plan(self, plan: List[Tuple[int, str, int]], unit: int = 1):\n",
    "        edges = self._group_edges()\n",
    "        G = len(edges)\n",
    "        applied = []\n",
    "        for (g, direction, cells) in plan:\n",
    "            last_idx_g = edges[g][1]\n",
    "            if direction == 'right':\n",
    "                moved = self._pop_right_push_left_across_boundary(last_idx_g, cells, unit=unit)\n",
    "            else:\n",
    "                moved = self._pop_left_push_right_across_boundary(last_idx_g, cells, unit=unit)\n",
    "            if moved > 0:\n",
    "                applied.append({\"g\": g, \"dir\": direction, \"cells_req\": cells, \"cells_moved\": moved, \"boundary_idx\": last_idx_g})\n",
    "        return applied\n",
    "\n",
    "    #Spiralheuristik innerhalb einer Gruppe: i, dann abwechselnd links/rechts ab i \n",
    "    def _neighbor_order_in_group(self, gi: int, i: int, x: float) -> List[int]:\n",
    "        members = sorted(self.groups[gi])\n",
    "        if i not in members:\n",
    "            i = min(members, key=lambda j: abs(j - i))\n",
    "        pos = members.index(i)\n",
    "        # Richtungspräferenz\n",
    "        Sb = self.S[i]\n",
    "        x_local = self.K * max(0.0, min(1.0, x)) - i\n",
    "        prefer_left = (x_local <= getattr(Sb, \"t\", 0.5)) if self.use_value_guided \\\n",
    "                      else (Sb.left.free() >= Sb.right.free())\n",
    "    \n",
    "        order = []\n",
    "        L = len(members)\n",
    "        d = 0\n",
    "        while True:\n",
    "            added = False\n",
    "            if d == 0:\n",
    "                order.append(members[pos]); added = True\n",
    "            else:\n",
    "                li = pos - d\n",
    "                ri = pos + d\n",
    "                if prefer_left:\n",
    "                    if li >= 0: order.append(members[li]); added = True\n",
    "                    if ri < L:  order.append(members[ri]); added = True\n",
    "                else:\n",
    "                    if ri < L:  order.append(members[ri]); added = True\n",
    "                    if li >= 0: order.append(members[li]); added = True\n",
    "            if not added:\n",
    "                break\n",
    "            d += 1\n",
    "        return order\n",
    "        \n",
    "    #Versuche Einfügen in Gruppenmitgliedern gemäß _neighbor_order_in_group (value-guided oder middle-pref)\n",
    "    def _insert_in_group_after_fairness(\n",
    "        self, gi: int, i: int, x: float) -> bool:\n",
    "        members = self._neighbor_order_in_group(gi, i, x)\n",
    "    \n",
    "        for j in members:\n",
    "            if self.use_value_guided:\n",
    "                if self.S[j].insert_value_guided(x, i=j, K=self.K):\n",
    "                    return True\n",
    "            else:\n",
    "                if self.S[j].insert_middle_pref(x):\n",
    "                    return True\n",
    "        return False\n",
    "\n",
    "    \n",
    "    @staticmethod\n",
    "    #Rundet v auf nächstes Vielfaches von unit, da wir unit=1 benutzen, macht das nichts, IGNORIEREN\n",
    "    def _round_to_multiple(v: int, unit: int) -> int:\n",
    "        if unit <= 1: return v\n",
    "        return unit * int(round(v / unit))\n",
    "\n",
    "    @staticmethod\n",
    "    #Korrigiert eine gerundete Delta-Liste, so dass die Summe genau 0 wird\n",
    "    def _fix_sum_to_zero(delta: List[int], unit: int) -> List[int]:\n",
    "        s = sum(delta)\n",
    "        if s == 0: return delta\n",
    "        d = delta[:]\n",
    "        while s != 0:\n",
    "            j = max(range(len(d)), key=lambda idx: abs(d[idx]))\n",
    "            if s > 0:\n",
    "                d[j] -= unit; s -= unit\n",
    "            else:\n",
    "                d[j] += unit; s += unit\n",
    "        return d\n",
    "        \n",
    "\n"
   ]
