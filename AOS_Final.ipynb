    "import math\n",
    "import random \n",
    "import numpy as np\n",
    "from tqdm import tqdm\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.stats import gaussian_kde\n",
    "from dataclasses import dataclass\n",
    "from typing import Callable, List, Optional, Tuple, Dict, Any\n",
    "from enum import Enum\n",
    "%run SortDet.ipynb\n",
    "\n",
    "#Phasen für die strenge Kontrolle des Einfügeflusses\n",
    "class Phase(Enum):\n",
    "    U_FILL = 1 #nur U(=vorderer Arrayteil) und Rebalance/Merge\n",
    "    B_FILL = 2 #alles nach B, kein Rebalance/Merge\n",
    "    U_AFTER_B = 3 #B voll -> nur noch in Löcher in U\n"
   ]
